
<div class="container">
    <div class="row mt-5 justify-content-center">
      <h4>注文履歴一覧</h4>
    </div>

    <table class="table table-hover table-bordered table-sm">
    <thead>
      <th>注文日時</th>
      <th>配送先名</th>
      <th>配送先</th>
      <th>ステータス</th>
    </thread>
</table>

    <body>
      <%= @orders.each do |order| %>
        <tr>
          <td><%= order.created_at.strftime('%Y年%m月%d日') %></td>
          <td><%= order.last_name + order.first_name %></td>
          <td><%= order.address %></td>
          <td></td>
        </tr>
      <% end %>
    </body>




